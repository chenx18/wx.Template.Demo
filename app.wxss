/**app.wxss**/
@import "weui.wxss";
@import "/assets/iconfont/iconfont.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.f-l {
  float: left;
}
.f-r {
  float: right;
}
/* 清除浮动 */
.clearfix:after{
  content:"";
  height:0;
  line-height:0;
  display:block;
  visibility:hidden;
  clear:both;
  }
  .clearfix{
    zoom:1;
  }